@model orkesterapp.Models.Orchester

@{
    ViewData["Title"] = "Register";
}

<h1>Register</h1>

<h4>Orchester</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Register">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="OrchestraName" class="control-label"></label>
                <input asp-for="OrchestraName" class="form-control" />
                <span asp-validation-for="OrchestraName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Register orchestra" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

