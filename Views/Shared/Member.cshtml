@model orkesterapp.Models.User

@{
    ViewData["Title"] = "Orchestra Member Page";
}

<div>
    <h4>Hello @Html.DisplayFor(model => model.FirstMidName) @Html.DisplayFor(model => model.LastName)</h4>
    @Html.ActionLink(
        linkText: "Details",
        actionName: "Details",
        controllerName: "User",
        routeValues: new {
            id = Model.ID
        },
        htmlAttributes: null
    )
    <br />
    <a>Logout</a>
    <hr>
    
    <div>
    @foreach (var item in Model.Orchester.Performances) {
            <tr>
                <td>
                    @item.Venue.VenueName
                </td>
                <td>
                    @item.Date
                </td>
            </tr>
    }
    </div>
</div>